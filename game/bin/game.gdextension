android.debug.x86_64 = "res://bin/android/libgame.android.template_debug.x86_64.so"
android.debug.x86_64.double = "res://bin/android/libgame.android.template_debug.double.x86_64.so"
android.release.x86_64 = "res://bin/android/libgame.android.template_release.x86_64.so"
android.release.x86_64.double = "res://bin/android/libgame.android.template_release.double.x86_64.so"

android.debug.arm64 = "res://bin/android/libgame.android.template_debug.arm64.so"
android.debug.arm64.double = "res://bin/android/libgame.android.template_debug.double.arm64.so"
android.release.arm64 = "res://bin/android/libgame.android.template_release.arm64.so"
