linux.debug.rv64 = "res://bin/linux/libgame.linux.template_debug.rv64.so"
linux.debug.rv64.double = "res://bin/linux/libgame.linux.template_debug.double.rv64.so"
linux.release.rv64 = "res://bin/linux/libgame.linux.template_release.rv64.so"
